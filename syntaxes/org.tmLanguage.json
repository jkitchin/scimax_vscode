{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Org Mode",
  "scopeName": "source.org",
  "patterns": [
    { "include": "#comments" },
    { "include": "#headings" },
    { "include": "#property-drawers" },
    { "include": "#drawers" },
    { "include": "#planning" },
    { "include": "#clock" },
    { "include": "#source-blocks" },
    { "include": "#example-blocks" },
    { "include": "#quote-blocks" },
    { "include": "#center-blocks" },
    { "include": "#verse-blocks" },
    { "include": "#export-blocks" },
    { "include": "#comment-blocks" },
    { "include": "#generic-blocks" },
    { "include": "#dynamic-blocks" },
    { "include": "#keywords" },
    { "include": "#horizontal-rule" },
    { "include": "#fixed-width" },
    { "include": "#tables" },
    { "include": "#lists" },
    { "include": "#footnote-definitions" },
    { "include": "#latex-environments" },
    { "include": "#inline-elements" }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.number-sign.org",
          "match": "^\\s*#(?!\\+).*$"
        }
      ]
    },
    "headings": {
      "patterns": [
        {
          "name": "markup.heading.1.org",
          "match": "^(\\*)\\s+(?:(TODO|DONE|NEXT|WAITING|HOLD|CANCELLED)\\s+)?(?:(\\[#[A-Za-z0-9]\\])\\s+)?(?:(COMMENT)\\s+)?(.*?)(\\s+:[\\w@#%:]+:)?\\s*$",
          "captures": {
            "1": { "name": "punctuation.definition.heading.1.org" },
            "2": { "name": "keyword.other.todo.org" },
            "3": { "name": "constant.other.priority.org" },
            "4": { "name": "comment.block.org" },
            "5": { "name": "entity.name.section.org", "patterns": [{ "include": "#inline-elements" }] },
            "6": { "name": "entity.name.tag.org" }
          }
        },
        {
          "name": "markup.heading.2.org",
          "match": "^(\\*\\*)\\s+(?:(TODO|DONE|NEXT|WAITING|HOLD|CANCELLED)\\s+)?(?:(\\[#[A-Za-z0-9]\\])\\s+)?(?:(COMMENT)\\s+)?(.*?)(\\s+:[\\w@#%:]+:)?\\s*$",
          "captures": {
            "1": { "name": "punctuation.definition.heading.2.org" },
            "2": { "name": "keyword.other.todo.org" },
            "3": { "name": "constant.other.priority.org" },
            "4": { "name": "comment.block.org" },
            "5": { "name": "entity.name.section.org", "patterns": [{ "include": "#inline-elements" }] },
            "6": { "name": "entity.name.tag.org" }
          }
        },
        {
          "name": "markup.heading.3.org",
          "match": "^(\\*\\*\\*)\\s+(?:(TODO|DONE|NEXT|WAITING|HOLD|CANCELLED)\\s+)?(?:(\\[#[A-Za-z0-9]\\])\\s+)?(?:(COMMENT)\\s+)?(.*?)(\\s+:[\\w@#%:]+:)?\\s*$",
          "captures": {
            "1": { "name": "punctuation.definition.heading.3.org" },
            "2": { "name": "keyword.other.todo.org" },
            "3": { "name": "constant.other.priority.org" },
            "4": { "name": "comment.block.org" },
            "5": { "name": "entity.name.section.org", "patterns": [{ "include": "#inline-elements" }] },
            "6": { "name": "entity.name.tag.org" }
          }
        },
        {
          "name": "markup.heading.4.org",
          "match": "^(\\*{4})\\s+(?:(TODO|DONE|NEXT|WAITING|HOLD|CANCELLED)\\s+)?(?:(\\[#[A-Za-z0-9]\\])\\s+)?(?:(COMMENT)\\s+)?(.*?)(\\s+:[\\w@#%:]+:)?\\s*$",
          "captures": {
            "1": { "name": "punctuation.definition.heading.4.org" },
            "2": { "name": "keyword.other.todo.org" },
            "3": { "name": "constant.other.priority.org" },
            "4": { "name": "comment.block.org" },
            "5": { "name": "entity.name.section.org", "patterns": [{ "include": "#inline-elements" }] },
            "6": { "name": "entity.name.tag.org" }
          }
        },
        {
          "name": "markup.heading.5.org",
          "match": "^(\\*{5})\\s+(?:(TODO|DONE|NEXT|WAITING|HOLD|CANCELLED)\\s+)?(?:(\\[#[A-Za-z0-9]\\])\\s+)?(?:(COMMENT)\\s+)?(.*?)(\\s+:[\\w@#%:]+:)?\\s*$",
          "captures": {
            "1": { "name": "punctuation.definition.heading.5.org" },
            "2": { "name": "keyword.other.todo.org" },
            "3": { "name": "constant.other.priority.org" },
            "4": { "name": "comment.block.org" },
            "5": { "name": "entity.name.section.org", "patterns": [{ "include": "#inline-elements" }] },
            "6": { "name": "entity.name.tag.org" }
          }
        },
        {
          "name": "markup.heading.6.org",
          "match": "^(\\*{6,})\\s+(?:(TODO|DONE|NEXT|WAITING|HOLD|CANCELLED)\\s+)?(?:(\\[#[A-Za-z0-9]\\])\\s+)?(?:(COMMENT)\\s+)?(.*?)(\\s+:[\\w@#%:]+:)?\\s*$",
          "captures": {
            "1": { "name": "punctuation.definition.heading.6.org" },
            "2": { "name": "keyword.other.todo.org" },
            "3": { "name": "constant.other.priority.org" },
            "4": { "name": "comment.block.org" },
            "5": { "name": "entity.name.section.org", "patterns": [{ "include": "#inline-elements" }] },
            "6": { "name": "entity.name.tag.org" }
          }
        }
      ]
    },
    "planning": {
      "patterns": [
        {
          "name": "meta.planning.org",
          "match": "^\\s*(SCHEDULED|DEADLINE|CLOSED):\\s*(.*)$",
          "captures": {
            "1": { "name": "keyword.other.planning.org" },
            "2": { "patterns": [{ "include": "#timestamps" }] }
          }
        }
      ]
    },
    "clock": {
      "patterns": [
        {
          "name": "meta.clock.org",
          "match": "^\\s*(CLOCK):\\s*(\\[.*?\\])(?:\\s*--\\s*(\\[.*?\\]))?(?:\\s*=>\\s*(\\d+:\\d+))?\\s*$",
          "captures": {
            "1": { "name": "keyword.other.clock.org" },
            "2": { "name": "constant.other.timestamp.inactive.org" },
            "3": { "name": "constant.other.timestamp.inactive.org" },
            "4": { "name": "constant.numeric.duration.org" }
          }
        }
      ]
    },
    "property-drawers": {
      "name": "meta.drawer.properties.org",
      "begin": "^(\\s*)(:PROPERTIES:)\\s*$",
      "end": "^(\\1)(:END:)\\s*$",
      "beginCaptures": {
        "2": { "name": "keyword.control.drawer.begin.org" }
      },
      "endCaptures": {
        "2": { "name": "keyword.control.drawer.end.org" }
      },
      "patterns": [
        {
          "name": "meta.property.org",
          "match": "^\\s*(:([A-Za-z_][A-Za-z0-9_-]*):)\\s*(.*)$",
          "captures": {
            "1": { "name": "entity.other.attribute-name.org" },
            "2": { "name": "variable.other.property.org" },
            "3": { "name": "string.unquoted.property-value.org", "patterns": [{ "include": "#timestamps" }] }
          }
        }
      ]
    },
    "drawers": {
      "name": "meta.drawer.org",
      "begin": "^(\\s*)(:([A-Za-z][A-Za-z0-9_-]*):)\\s*$",
      "end": "^(\\1)(:END:)\\s*$",
      "beginCaptures": {
        "2": { "name": "keyword.control.drawer.begin.org" },
        "3": { "name": "entity.name.drawer.org" }
      },
      "endCaptures": {
        "2": { "name": "keyword.control.drawer.end.org" }
      },
      "patterns": [
        { "include": "#inline-elements" }
      ]
    },
    "source-blocks": {
      "patterns": [
        {
          "name": "meta.embedded.block.python.org",
          "begin": "(?i)^(\\s*)(#\\+BEGIN_SRC)\\s+(python)(.*)$",
          "end": "(?i)^(\\1)(#\\+END_SRC)\\s*$",
          "beginCaptures": {
            "2": { "name": "keyword.control.block.begin.org" },
            "3": { "name": "storage.type.language.org" },
            "4": { "name": "meta.block.header.org" }
          },
          "endCaptures": {
            "2": { "name": "keyword.control.block.end.org" }
          },
          "contentName": "meta.embedded.block.python",
          "patterns": [{ "include": "source.python" }]
        },
        {
          "name": "meta.embedded.block.bash.org",
          "begin": "(?i)^(\\s*)(#\\+BEGIN_SRC)\\s+(bash|sh|shell)(.*)$",
