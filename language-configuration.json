{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["<", ">"]
  ],
  "autoClosingPairs": [
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "{", "close": "}" },
    { "open": "<", "close": ">" },
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "'", "close": "'", "notIn": ["string"] },
    { "open": "*", "close": "*" },
    { "open": "/", "close": "/" },
    { "open": "_", "close": "_" },
    { "open": "~", "close": "~" },
    { "open": "=", "close": "=" },
    { "open": "+", "close": "+" }
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["<", ">"],
    ["\"", "\""],
    ["'", "'"],
    ["*", "*"],
    ["/", "/"],
    ["_", "_"],
    ["~", "~"],
    ["=", "="],
    ["+", "+"]
  ],
  "folding": {
    "markers": {
      "start": "^\\*+ |^[ \\t]*#\\+BEGIN_",
      "end": "^[ \\t]*#\\+END_"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "^[ \\t]*[-+*]|^[ \\t]*\\d+[.)]",
    "decreaseIndentPattern": "^[ \\t]*$"
  },
  "wordPattern": "[a-zA-Z0-9_\\-]+"
}
